#: ../../reference/views.rst:1180
msgid "declares a sub view."
msgstr ""

#: ../../reference/views.rst:1182
#: ../../reference/views.rst:1199
#: ../../reference/views.rst:1219
#: ../../reference/views.rst:1269
#: ../../reference/views.rst:1295
msgid "Admissible attributes are:"
msgstr ""

#: ../../reference/views.rst:1185
msgid "``type`` (mandatory)"
msgstr ""

#: ../../reference/views.rst:1185
msgid "The type of the sub view.  For example, *graph* or *pivot*."
msgstr ""

#: ../../reference/views.rst:1189
msgid "``ref`` (optional)"
msgstr ""

#: ../../reference/views.rst:1188
msgid "An xml id for a view. If not given, the default view for the model will be used."
msgstr ""

#: ../../reference/views.rst:1193
#: ../../reference/views.rst:1278
msgid "``name`` (optional)"
msgstr ""

#: ../../reference/views.rst:1192
msgid "A string which identifies this element.  It is mostly useful to be used as a target for an xpath."
